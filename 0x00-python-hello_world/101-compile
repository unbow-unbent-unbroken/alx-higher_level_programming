#!/bin/bash
# Check if $PYFILE is set
if [ -z "$PYFILE" ]; then
    echo "Error: Environment variable \$PYFILE is not set."
    exit 1
fi

# Check if the Python file exists
if [ ! -f "$PYFILE" ]; then
    echo "Error: Python file '$PYFILE' not found."
    exit 1
fi

# Compile the Python script
python -m py_compile "$PYFILE"
